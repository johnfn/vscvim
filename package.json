{
	"name": "vscvim",
	"description": "vim extension for vsc",
	"version": "0.0.1",
	"publisher": "johnfn",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/vscvim",
	"contributes": {
		"keybindings": [
			{ "command": "extension.press_i",      "key": "i"      },
			{ "command": "extension.press_escape", "key": "escape" },
			{ "command": "extension.press_h",      "key": "h"      },
			{ "command": "extension.press_l",      "key": "l"      },
      { "command": "extension.press_j",      "key": "j"      },
      { "command": "extension.press_k",      "key": "k"      },
      { "command": "extension.press_w",      "key": "w"      },
      { "command": "extension.press_b",      "key": "b"      },
      { "command": "extension.press_v",      "key": "v"      },
			{ "command": "extension.press_d",      "key": "d"      },
			{ "command": "extension.press_c",      "key": "c"      }
    ]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}
